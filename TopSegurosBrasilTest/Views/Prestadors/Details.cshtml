@model TopSegurosBrasil.Models.Prestador

@{
    ViewData["Title"] = "Create";
}

<h1 class="mb-4 text-3xl font-extrabold tracking-tight leading-none text-gray-900">Criar Prestador</h1>

<nav class="flex px-5 py-3 text-gray-700 border border-gray-200 rounded-lg bg-gray-50 mb-8" aria-label="Breadcrumb">
    <ol class="inline-flex items-center space-x-1 md:space-x-3">
        <li class="inline-flex items-center">
            <a href="#" class="inline-flex items-center text-sm font-medium text-gray-700 hover:text-gray-900">
                <svg class="w-4 h-4 mr-2" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path d="M10.707 2.293a1 1 0 00-1.414 0l-7 7a1 1 0 001.414 1.414L4 10.414V17a1 1 0 001 1h2a1 1 0 001-1v-2a1 1 0 011-1h2a1 1 0 011 1v2a1 1 0 001 1h2a1 1 0 001-1v-6.586l.293.293a1 1 0 001.414-1.414l-7-7z"></path></svg>
                Home
            </a>
        </li>
        <li>
            <div class="flex items-center">
                <svg class="w-6 h-6 text-gray-400" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z" clip-rule="evenodd"></path></svg>
                <a href="#" class="ml-1 text-sm font-medium text-gray-700 hover:text-gray-900 md:ml-2">Tabela Clientes</a>
            </div>
        </li>
        <li aria-current="page">
            <div class="flex items-center">
                <svg class="w-6 h-6 text-gray-400" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z" clip-rule="evenodd"></path></svg>
                <span class="ml-1 text-sm font-medium text-blue-600 md:ml-2">Criar Prestador</span>
            </div>
        </li>
    </ol>
</nav>

<div>
    <div class="grid gap-6 mb-6 md:grid-cols-2">
        <div>
            <label class="labelCreateClients">Nome da Empresa</label>
            <input value="@Html.DisplayFor(model => model.NomeEmpresa)" class="inputCreateClients" disabled />
        </div>
        <div>
            <label class="labelCreateClients">CNPJ</label>
            <input value="@Html.DisplayFor(model => model.Cnpj)" class="inputCreateClients" disabled />
        </div>
        <div>
            <label for="TipoPrestador" class="labelCreateClients">Tipo do Prestador</label>
            <input value="@Html.DisplayFor(model => model.TipoPrestador)" class="inputCreateClients" disabled />
        </div>
        <div>
            <label class="labelCreateClients">Email</label>
            <input value="@Html.DisplayFor(model => model.Email)" class="inputCreateClients" disabled />
        </div>
        <div>
            <label class="labelCreateClients">Telefone</label>
            <input value="@Html.DisplayFor(model => model.Telefone)" class="inputCreateClients" disabled />
        </div>
        <div>
            <label for="Estado" class="labelCreateClients">Estado</label>
            <input value="@Html.DisplayFor(model => model.Estado)" class="inputCreateClients" disabled />
        </div>

        <div>
            <label class="labelCreateClients">Cidade</label>
            <input value="@Html.DisplayFor(model => model.Cidade)" class="inputCreateClients" disabled />
        </div>
        <div>
            <label class="labelCreateClients">Bairro</label>
            <input value="@Html.DisplayFor(model => model.Bairro)" class="inputCreateClients" disabled />
        </div>
        <div>
            <label class="labelCreateClients">CEP</label>
            <input value="@Html.DisplayFor(model => model.Cep)" class="inputCreateClients" disabled />
        </div>
        <div>
            <label class="labelCreateClients">Numero do imóvel</label>
            <input value="@Html.DisplayFor(model => model.Num_Imovel)" class="inputCreateClients" disabled />
        </div>
    </div>
    <div>
        <a asp-action="Index" class="underline font-medium text-blue-600 dark:text-blue-500 hover:underline">Voltar para lista</a>
    </div>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    }
